<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="pane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@DataViewStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.pc.DataViewController">
    <ImageView fx:id="back" fitHeight="50.0" fitWidth="40.0" onMouseClicked="#clickBack" pickOnBounds="true">
        <Image url="@image/data/back.png" />
    </ImageView>
    <Label fx:id="name" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
    <Label fx:id="school" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
    <Label fx:id="score" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
    <Label fx:id="totalTime" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="4" />
    <Label fx:id="totalTimes" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5" />
    <ImageView fx:id="timePieChartController" fitHeight="35.0" onMouseEntered="#enterTimePieChartController" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="7">
        <Image url="@image/data/timePieChart.png"/>
    </ImageView>
    <ImageView fx:id="timesPieChartController" fitHeight="35.0" onMouseEntered="#enterTimesPieChartController" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="7">
        <Image url="@image/data/timesPieChart.png"/>
    </ImageView>
    <ImageView fx:id="timeBarChartController" fitHeight="35.0" onMouseEntered="#enterTimeBarChartController" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="8">
        <Image url="@image/data/timeBarChart.png"/>
    </ImageView>
    <ImageView fx:id="timesBarChartController" fitHeight="35.0" onMouseEntered="#enterTimesBarChartController" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="8">
        <Image url="@image/data/timesBarChart.png"/>
    </ImageView>
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" percentWidth="5.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="10.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="10.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="10.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="60.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="5.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="5.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="15.0" valignment="CENTER" />
        <RowConstraints percentHeight="15.0" valignment="CENTER" />
        <RowConstraints percentHeight="5.0" valignment="CENTER" />
    </rowConstraints>
</GridPane>
