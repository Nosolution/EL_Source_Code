<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<GridPane prefHeight="400.0" prefWidth="600.0" stylesheets="@PauseViewStyle.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.pc.PauseViewController">
    <Label fx:id="time" text="3:00" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Text fx:id="back" onMouseClicked="#clickBack" strokeType="OUTSIDE" strokeWidth="0.0" text="Continue" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" percentWidth="3.0" />
        <ColumnConstraints halignment="CENTER" percentWidth="97.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="10.0" valignment="CENTER" />
        <RowConstraints percentHeight="20.0" valignment="CENTER" />
        <RowConstraints percentHeight="50.0" valignment="CENTER" />
        <RowConstraints percentHeight="20.0" valignment="CENTER" />
    </rowConstraints>
</GridPane>
