<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7793cb">

    <TextView
        android:id="@+id/text_title_developers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="46dp"
        android:layout_toEndOf="@+id/imageButton_developersList"
        android:layout_toRightOf="@+id/imageButton_developersList"
        android:text="开发者团队"
        android:textColor="#0b1834"
        android:textSize="26sp" />

    <ImageButton
        android:id="@+id/imageButton_developersList"
        android:layout_width="37dp"
        android:layout_height="37dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/text_title_developers"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/exit_developers" />

    <TextView
        android:id="@+id/line_1"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/text_title_developers"
        android:layout_marginTop="14dp"
        android:background="#304573" />

    <ImageView
        android:id="@+id/imageView_buttomPic"
        android:layout_width="428dp"
        android:layout_height="175dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-16dp"
        app:srcCompat="@drawable/dev_buttom_pic" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:textColor="#3A4A6B"
        android:textSize="18sp"
        android:text="软件设计" />

    <TextView
        android:id="@+id/line_2"
        android:layout_width="200dp"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
        android:background="#51648f" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp"
        android:textColor="#1b2c4e"
        android:textSize="16sp"
        android:text="曾思鸿  余炳蔚" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="#3A4A6B"
        android:textSize="18sp"
        android:text="功能开发" />

    <TextView
        android:id="@+id/line_3"
        android:layout_width="200dp"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
        android:background="#51648f" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp"
        android:textColor="#1b2c4e"
        android:textSize="16sp"
        android:text="储梓建  金魏向若  余炳蔚  曾思鸿" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="#3A4A6B"
        android:textSize="18sp"
        android:text="美工设计" />

    <TextView
        android:id="@+id/line_4"
        android:layout_width="200dp"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
        android:background="#51648f" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp"
        android:textColor="#1b2c4e"
        android:textSize="16sp"
        android:text="曾思鸿" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="#3A4A6B"
        android:textSize="18sp"
        android:text="调试测试" />

    <TextView
        android:id="@+id/line_5"
        android:layout_width="200dp"
        android:layout_height="0.5dp"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
        android:background="#51648f" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line_5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp"
        android:textColor="#1b2c4e"
        android:textSize="16sp"
        android:text="储梓建  金魏向若  余炳蔚  曾思鸿" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="24dp"
        android:text="Copyright @2018 NJUSE  ELTeam."
        android:textColor="#ffe26d"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"
        android:textColor="#ffe26d"
        android:textSize="12sp"
        android:text="All Rights Reserved." />

</RelativeLayout>